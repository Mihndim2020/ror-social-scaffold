<section class="users-section">
  <ul class="users-list">
    <% @users.each do |user| %>
      <li class="profile-item">
     
        <span>
        
          <%= link_to user.name,  user_path(user), class: 'profile-link' %>
          <%= button_display(user) %>
        </span>
      </li>
    <% end %>
  </ul>
</section>
